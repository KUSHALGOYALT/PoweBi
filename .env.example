# PowerBI Configuration (Sample)
# Copy this file to .env and fill with your actual values

CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here
TENANT_ID=your_tenant_id_here

# PowerBI Report Details
POWERBI_WORKSPACE_ID=your_workspace_id_here
POWERBI_REPORT_ID=your_report_id_here

# PowerBI URLs
POWERBI_AUTHORITY_URL=https://login.microsoftonline.com/your_tenant_id
POWERBI_SCOPE=https://analysis.windows.net/powerbi/api/.default
POWERBI_API_URL=https://api.powerbi.com/v1.0/myorg
